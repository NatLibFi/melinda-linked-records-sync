{
    "name": "@natlibfi/todo",
    "version": "0.0.1",
    "description": "",
    "author": {
        "name": "The National Library of Finland"
    },
    "repository": {
        "type": "git",
        "url": "TODO"
    },
    "homepage": "TODO",
    "bugs": {
        "url": "https://todo.com/"
    },
    "license": "AGPL-3.0+",
    "engines": {
        "node": ">=12"
    },
    "main": "src/index.js",
    "scripts": {
        "start": "node ./dist/index",
        "start:dev": "babel-node ./src/index",
        "lint": "eslint src",
        "lint:dev": "eslint --fix src",
        "check-deps": "npm-check;exit 0",
        "test": "npm run lint",
        "dev": "NODE_ENV=development cross-env LOG_LEVEL=silly nodemon",
        "build": "babel src --source-maps --copy-files --delete-dir-on-start --out-dir=dist --minified",
        "prod": "NODE_ENV=production npm run build && npm run start"
    },
    "dependencies": {
        "@natlibfi/marc-record-serializers": "6.2.0",
        "@natlibfi/marc-record-validate": "5.0.1",
        "@natlibfi/marc-record-validators-melinda": "8.2.3",
        "@natlibfi/melinda-commons": "git+https://github.com/NatLibFi/melinda-commons-js.git#melkehitys-1541",
        "@natlibfi/melinda-record-link-migration-commons": "git+https://github.com/NatLibFi/melinda-record-link-migration-commons.git#melkehitys-1117",
        "@natlibfi/oai-pmh-client": "2.0.0",
        "amqplib": ">=0.5.5 < 1.0.0",
        "esm": "^3.2.25",
        "moment": "^2.27.0",
        "mongodb": "^3.5.3",
        "node-fetch": "2.6.0",
        "uuid": "^8.2.0",
        "xml2js": ">=0.4.23 < 1.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.7",
        "@babel/node": "^7.8.7",
        "@babel/preset-env": "^7.8.7",
        "babel-eslint": "^10.0.3",
        "cross-env": "^7.0.0",
        "eslint": "^6.8.0",
        "@natlibfi/eslint-config-melinda-backend": ">1.0.0-alpha.1",
        "eslint-plugin-import": "^2.20.0",
        "nodemon": "^2.0.2",
        "npm-check": "^5.9.0"
    },
    "eslintConfig": {
        "extends": "@natlibfi/melinda-backend"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "12"
                    }
                }
            ]
        ]
    },
    "nodemonConfig": {
        "exec": "npm run lint:dev && npm run start:dev",
        "watch": [
            "src/*"
        ],
        "ext": "js",
        "delay": 2.5
    }
}
